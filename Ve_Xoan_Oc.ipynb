   "source": [
    "import turtle\n",
    "t = turtle.Turtle()\n",
    "t.speed(100)\n",
    "t.pensize(2)\n",
    "t.pencolor(\"black\")\n",
    "t.right(10)\n",
    "d = 0\n",
    "r = 0\n",
    "while r<200:\n",
    "    t.forward(d)\n",
    "    t.left(10)\n",
    "    d += 0.1\n",
    "    r += 1        \n",
    "turtle.done()"
   ]
  }
